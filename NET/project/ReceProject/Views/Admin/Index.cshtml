@using Microsoft.AspNetCore.Identity
@inject SignInManager<IdentityUser> SignInManager
@inject UserManager<IdentityUser> UserManager

@{
    ViewData["Title"] = "Admin Page";
}


<h1>Admin</h1>
<p>Inloggad som: @User.Identity?.Name </p>
<a asp-controller="Home" asp-action="Index">Hoppa till den publika webbplatsen</a>


<form class="logoutForm" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
    <input type="submit" value="Logga ut" />
</form>

