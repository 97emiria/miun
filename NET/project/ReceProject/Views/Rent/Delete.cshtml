@model ReceProject.Models.Rent

@{
    ViewData["Title"] = "Delete";
}

<h1>Ta bort hyrning för</h1>

<p class="admin_deleteText"> 
    Hyrning för
    <strong> @Html.DisplayFor(model => model.Name)</strong>    
    av rummet 
    <strong>@Html.DisplayFor(model => model.Room.Name)</strong> 
    kommer att tas bort
</p>

<form asp-action="Delete" >
    <input type="hidden" asp-for="Id" />
    <input type="submit" value="Ta bort" class="btn btn-danger" />
</form>
